package ysit.vo;

/**
 * 학생의 재적상태(휴학/자퇴)를 저장하는 VO 
 *
 */

public class ApplyVO {
	
	// ApplyVO
	private String app_id;							// 신청코드[PK]
	private String admin_id;						// 사번(관리자 아이디)[FK]
	private String stu_id;							// 학번(학생 아이디)[FK]
	private String app_content;						// 신청내용
	private String app_date;						// 신청일
	private String app_pro;							// 승인여부_교수
	private String app_approval_date;				// 승인일_교수
	private String app_adm;							// 승인여부_관리자
	private String app_adm_approval_date;			// 승인일_관리자
	private String coun_id;							// 상담코드[FK]
	private String app_rejection_date;				// 신청 거절일
	private String app_reasons_rejection;			// 신청 반려사유
	
	
	// getter, setter
	public String getApp_id() {
		return app_id;
	}
	public void setApp_id(String app_id) {
		this.app_id = app_id;
	}
	public String getAdmin_id() {
		return admin_id;
	}
	public void setAdmin_id(String admin_id) {
		this.admin_id = admin_id;
	}
	public String getStu_id() {
		return stu_id;
	}
	public void setStu_id(String stu_id) {
		this.stu_id = stu_id;
	}
	public String getApp_content() {
		return app_content;
	}
	public void setApp_content(String app_content) {
		this.app_content = app_content;
	}
	public String getApp_date() {
		return app_date;
	}
	public void setApp_date(String app_date) {
		this.app_date = app_date;
	}
	public String getApp_pro() {
		return app_pro;
	}
	public void setApp_pro(String app_pro) {
		this.app_pro = app_pro;
	}
	public String getApp_approval_date() {
		return app_approval_date;
	}
	public void setApp_approval_date(String app_approval_date) {
		this.app_approval_date = app_approval_date;
	}
	public String getApp_adm() {
		return app_adm;
	}
	public void setApp_adm(String app_adm) {
		this.app_adm = app_adm;
	}
	public String getApp_adm_approval_date() {
		return app_adm_approval_date;
	}
	public void setApp_adm_approval_date(String app_adm_approval_date) {
		this.app_adm_approval_date = app_adm_approval_date;
	}
	public String getCoun_id() {
		return coun_id;
	}
	public void setCoun_id(String coun_id) {
		this.coun_id = coun_id;
	}
	public String getApp_rejection_date() {
		return app_rejection_date;
	}
	public void setApp_rejection_date(String app_rejection_date) {
		this.app_rejection_date = app_rejection_date;
	}
	public String getApp_reasons_rejection() {
		return app_reasons_rejection;
	}
	public void setApp_reasons_rejection(String app_reasons_rejection) {
		this.app_reasons_rejection = app_reasons_rejection;
	}
	
	
	// toString
	@Override
	public String toString() {
		return "ApplyVO [app_id=" + app_id + ", admin_id=" + admin_id + ", stu_id=" + stu_id + ", app_content="
				+ app_content + ", app_date=" + app_date + ", app_pro=" + app_pro + ", app_approval_date="
				+ app_approval_date + ", app_adm=" + app_adm + ", app_adm_approval_date=" + app_adm_approval_date
				+ ", coun_id=" + coun_id + ", app_rejection_date=" + app_rejection_date + ", app_reasons_rejection="
				+ app_reasons_rejection + "]";
	}

	
	
	
}
