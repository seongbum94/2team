package ysit.com.login.controller;

import java.io.UnsupportedEncodingException;
import java.net.URL;
import java.util.ResourceBundle;

import javax.mail.MessagingException;

import javafx.event.ActionEvent;
import javafx.fxml.FXML;
import javafx.scene.control.Button;
import ysit.api.SendEmail;

public class PassWrodController {

    @FXML
    private ResourceBundle resources;

    @FXML
    private URL location;

    @FXML
    private Button btnOk;

    @FXML
    void onClickOk(ActionEvent event) throws UnsupportedEncodingException, MessagingException {
    	SendEmail send = new SendEmail();
    	send.main(null);
    }

    @FXML
    void initialize() {
        assert btnOk != null : "fx:id=\"btnOk\" was not injected: check your FXML file 'PassWord.fxml'.";

    }
}
