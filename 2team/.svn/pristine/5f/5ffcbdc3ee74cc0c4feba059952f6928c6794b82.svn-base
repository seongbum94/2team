package ysit.admin.mstudent.service;

import java.util.List;

import ysit.vo.StudentVO;

public interface IMStudentService {
	/**
	 *  교수 전체 출력 메서드
	 *  @return List<StudentVO> 학생 정보 전부 + 학과명
	 */
	public List<StudentVO> getAllStudent();
	
	/**
	 *  학생이름 검색 메서드
	 *  @param 검색할 학생이름
	 *  @return List<StudentVO> 이름으로 검색된 학생 정보
	 */
	public List<StudentVO> getStuName(String stu_name);
	
	
	/**
	 *  학번 검색 메서드
	 *  @param 검색할 학번
	 *  @return List<StudentVO> 학번으로 검색된 학생 정보
	 */
	public List<StudentVO> getStuId(String stu_id);

}
