package ysit.vo;

/**
 * 개설된 과목의 정보를 저장하는 VO 
 */

public class OpeningSubjectVO {
	private String opsub_id;			// 개설코드[PK]
	private String opsub_year;			// 개설년도
	private int opsub_sem;				// 학기
	private String sub_id;				// 과목코드[FK]
	private int opsub_credit;			// 학점
	private String pro_id;				// 교번[FK]
	private int opsub_price;			// 금액
	private int opsub_limit_people;		// 제한인원
	private int sub_member;				// 수강인원
	private int sub_room;				// 강의실번호
	private String pro_plan;			// 강의계획서 파일경로
	private String sub_stat;			// 강의상태
	
	
	// getter, setter
	public String getOpsub_id() {
		return opsub_id;
	}
	public void setOpsub_id(String opsub_id) {
		this.opsub_id = opsub_id;
	}
	public String getOpsub_year() {
		return opsub_year;
	}
	public void setOpsub_year(String opsub_year) {
		this.opsub_year = opsub_year;
	}
	public int getOpsub_sem() {
		return opsub_sem;
	}
	public void setOpsub_sem(int opsub_sem) {
		this.opsub_sem = opsub_sem;
	}
	public String getSub_id() {
		return sub_id;
	}
	public void setSub_id(String sub_id) {
		this.sub_id = sub_id;
	}
	public int getOpsub_credit() {
		return opsub_credit;
	}
	public void setOpsub_credit(int opsub_credit) {
		this.opsub_credit = opsub_credit;
	}
	public String getPro_id() {
		return pro_id;
	}
	public void setPro_id(String pro_id) {
		this.pro_id = pro_id;
	}
	public int getOpsub_price() {
		return opsub_price;
	}
	public void setOpsub_price(int opsub_price) {
		this.opsub_price = opsub_price;
	}
	public int getOpsub_limit_people() {
		return opsub_limit_people;
	}
	public void setOpsub_limit_people(int opsub_limit_people) {
		this.opsub_limit_people = opsub_limit_people;
	}
	public int getSub_member() {
		return sub_member;
	}
	public void setSub_member(int sub_member) {
		this.sub_member = sub_member;
	}
	public int getSub_room() {
		return sub_room;
	}
	public void setSub_room(int sub_room) {
		this.sub_room = sub_room;
	}
	public String getPro_plan() {
		return pro_plan;
	}
	public void setPro_plan(String pro_plan) {
		this.pro_plan = pro_plan;
	}
	public String getSub_stat() {
		return sub_stat;
	}
	public void setSub_stat(String sub_stat) {
		this.sub_stat = sub_stat;
	}
	
	// toString
	@Override
	public String toString() {
		return "OpeningSubjectVO [opsub_id=" + opsub_id + ", opsub_year=" + opsub_year + ", opsub_sem=" + opsub_sem
				+ ", sub_id=" + sub_id + ", opsub_credit=" + opsub_credit + ", pro_id=" + pro_id + ", opsub_price="
				+ opsub_price + ", opsub_limit_people=" + opsub_limit_people + ", sub_member=" + sub_member
				+ ", sub_room=" + sub_room + ", pro_plan=" + pro_plan + ", sub_stat=" + sub_stat + "]";
	}

}
