package ysit.vo;

/**
 * 등록금 정보를 저장하는 VO 
 *
 */

public class TuitionVO {
	private String tuit_id;						// 등록금번호[PK]	
	private String opsub_year;					// 개설년도
	private String tuit_sem;					// 학번[FK]
	private String stu_id;						// 학기
	private int tuit_money;						// 등록금
	private String tuit_ok;						// 납부여부
	private String tuit_card_number;			// 카드번호
	private int tuit_payment_amount;			// 결제금액
	private String tuit_refund_account;			// 환불계좌
	private String tuit_bank;					// 은행명
	private String tuit_account_holder;			// 예금주
	private String tuit_refund_date;			// 환불일시

	
	// getter, setter
	public String getTuit_id() {
		return tuit_id;
	}
	public void setTuit_id(String tuit_id) {
		this.tuit_id = tuit_id;
	}
	public String getOpsub_year() {
		return opsub_year;
	}
	public void setOpsub_year(String opsub_year) {
		this.opsub_year = opsub_year;
	}
	public String getTuit_sem() {
		return tuit_sem;
	}
	public void setTuit_sem(String tuit_sem) {
		this.tuit_sem = tuit_sem;
	}
	public String getStu_id() {
		return stu_id;
	}
	public void setStu_id(String stu_id) {
		this.stu_id = stu_id;
	}
	public int getTuit_money() {
		return tuit_money;
	}
	public void setTuit_money(int tuit_money) {
		this.tuit_money = tuit_money;
	}
	public String getTuit_ok() {
		return tuit_ok;
	}
	public void setTuit_ok(String tuit_ok) {
		this.tuit_ok = tuit_ok;
	}
	public String getTuit_card_number() {
		return tuit_card_number;
	}
	public void setTuit_card_number(String tuit_card_number) {
		this.tuit_card_number = tuit_card_number;
	}
	public int getTuit_payment_amount() {
		return tuit_payment_amount;
	}
	public void setTuit_payment_amount(int tuit_payment_amount) {
		this.tuit_payment_amount = tuit_payment_amount;
	}
	public String getTuit_refund_account() {
		return tuit_refund_account;
	}
	public void setTuit_refund_account(String tuit_refund_account) {
		this.tuit_refund_account = tuit_refund_account;
	}
	public String getTuit_bank() {
		return tuit_bank;
	}
	public void setTuit_bank(String tuit_bank) {
		this.tuit_bank = tuit_bank;
	}
	public String getTuit_account_holder() {
		return tuit_account_holder;
	}
	public void setTuit_account_holder(String tuit_account_holder) {
		this.tuit_account_holder = tuit_account_holder;
	}
	public String getTuit_refund_date() {
		return tuit_refund_date;
	}
	public void setTuit_refund_date(String tuit_refund_date) {
		this.tuit_refund_date = tuit_refund_date;
	}
	
	
	// toString
	@Override
	public String toString() {
		return "TuitionVO [tuit_id=" + tuit_id + ", opsub_year=" + opsub_year + ", tuit_sem=" + tuit_sem + ", stu_id="
				+ stu_id + ", tuit_money=" + tuit_money + ", tuit_ok=" + tuit_ok + ", tuit_card_number="
				+ tuit_card_number + ", tuit_payment_amount=" + tuit_payment_amount + ", tuit_refund_account="
				+ tuit_refund_account + ", tuit_bank=" + tuit_bank + ", tuit_account_holder=" + tuit_account_holder
				+ ", tuit_refund_date=" + tuit_refund_date + "]";
	}

}
