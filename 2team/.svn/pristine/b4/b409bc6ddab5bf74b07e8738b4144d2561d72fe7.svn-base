<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="636.0" prefWidth="831.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ysit.professor.counsel.controller.Pro_counController">
   <center>
      <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn fx:id="appDate" prefWidth="104.0" text="신청일자" />
          <TableColumn fx:id="major" prefWidth="124.0" text="소속학과" />
          <TableColumn fx:id="year" prefWidth="69.0" text="학년" />
            <TableColumn fx:id="stuId" prefWidth="96.0" text="학번" />
            <TableColumn fx:id="stuName" prefWidth="99.0" text="학생명" />
            <TableColumn fx:id="appReason" prefWidth="255.0" text="신청사유" />
            <TableColumn fx:id="aprType" prefWidth="83.0" text="승인여부" />
        </columns>
      </TableView>
   </center>
   <top>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <DatePicker fx:id="counDate" />
            <Button fx:id="btnSearch" mnemonicParsing="false" onAction="#searchOnClick" text="검색" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
