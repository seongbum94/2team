package ysit.vo;

/**
 * 학생 정보를 저장하는 VO 
 *
 */


public class StudentVO {
	private String stu_id;							// 학번(학생아이디) [PK]
	private String stu_pass;						// 비밀번호
	private String stu_photo;						// 학생 사진
	private String major_id;						// 학생 소속학과코드 [FK]
	private String stu_name;						// 학생 이름	
	private String stu_ent;							// 학생 입학일
	private String stu_regno1;						// 학생 주민등록번호 앞자리
	private String stu_regno2;						// 학생 주민등록번호 뒷자리
	private String stu_register;					// 학생 학적상태
	private String stu_tel;							// 학생 전화번호
	private String stu_mail;						// 학생 이메일
	private String stu_addr;						// 학생 주소
	private int stu_warn;							// 학생 학사경고
	private int stu_credits_point;					// 학생 현재 이수학점
	private int stu_graduation_minimum_credits;		// 학생 졸업최소이수학점
	private int stu_grant;							// 학생 권한 (2)
	
	private String major_name;                      //학과명 (join결과)
	
	private String stu_bankName;                    //은행명
	private String stu_account;                     //환불계좌번호
	private String stu_accountHolder;               //예금주
	
	
	
	// getter, setter
	public String getStu_id() {
		return stu_id;
	}
	public void setStu_id(String stu_id) {
		this.stu_id = stu_id;
	}
	public String getStu_pass() {
		return stu_pass;
	}
	public void setStu_pass(String stu_pass) {
		this.stu_pass = stu_pass;
	}
	public String getStu_photo() {
		return stu_photo;
	}
	public void setStu_photo(String stu_photo) {
		this.stu_photo = stu_photo;
	}
	public String getMajor_id() {
		return major_id;
	}
	public void setMajor_id(String major_id) {
		this.major_id = major_id;
	}
	public String getStu_name() {
		return stu_name;
	}
	public void setStu_name(String stu_name) {
		this.stu_name = stu_name;
	}
	public String getStu_ent() {
		return stu_ent;
	}
	public void setStu_ent(String stu_ent) {
		this.stu_ent = stu_ent;
	}
	public String getStu_regno1() {
		return stu_regno1;
	}
	public void setStu_regno1(String stu_regno1) {
		this.stu_regno1 = stu_regno1;
	}
	public String getStu_regno2() {
		return stu_regno2;
	}
	public void setStu_regno2(String stu_regno2) {
		this.stu_regno2 = stu_regno2;
	}
	public String getStu_register() {
		return stu_register;
	}
	public void setStu_register(String stu_register) {
		this.stu_register = stu_register;
	}
	public String getStu_tel() {
		return stu_tel;
	}
	public void setStu_tel(String stu_tel) {
		this.stu_tel = stu_tel;
	}
	public String getStu_mail() {
		return stu_mail;
	}
	public void setStu_mail(String stu_mail) {
		this.stu_mail = stu_mail;
	}
	public String getStu_addr() {
		return stu_addr;
	}
	public void setStu_addr(String stu_addr) {
		this.stu_addr = stu_addr;
	}
	public int getStu_warn() {
		return stu_warn;
	}
	public void setStu_warn(int stu_warn) {
		this.stu_warn = stu_warn;
	}
	public int getStu_credits_point() {
		return stu_credits_point;
	}
	public void setStu_credits_point(int stu_credits_point) {
		this.stu_credits_point = stu_credits_point;
	}
	public int getStu_graduation_minimum_credits() {
		return stu_graduation_minimum_credits;
	}
	public void setStu_graduation_minimum_credits(int stu_graduation_minimum_credits) {
		this.stu_graduation_minimum_credits = stu_graduation_minimum_credits;
	}
	public int getStu_grant() {
		return stu_grant;
	}
	public void setStu_grant(int stu_grant) {
		this.stu_grant = stu_grant;
	}
	public String getMajor_name() {
		return major_name;
	}
	public void setMajor_name(String major_name) {
		this.major_name = major_name;
	}
	
	public String getStu_bankName() {
		return stu_bankName;
	}
	public void setStu_bankName(String stu_bankName) {
		this.stu_bankName = stu_bankName;
	}
	public String getStu_account() {
		return stu_account;
	}
	public void setStu_account(String stu_account) {
		this.stu_account = stu_account;
	}
	public String getStu_accountHolder() {
		return stu_accountHolder;
	}
	public void setStu_accountHolder(String stu_accountHolder) {
		this.stu_accountHolder = stu_accountHolder;
	}
	
	//to String
	@Override
	public String toString() {
		return "StudentVO [stu_id=" + stu_id + ", stu_pass=" + stu_pass + ", stu_photo=" + stu_photo + ", major_id="
				+ major_id + ", stu_name=" + stu_name + ", stu_ent=" + stu_ent + ", stu_regno1=" + stu_regno1
				+ ", stu_regno2=" + stu_regno2 + ", stu_register=" + stu_register + ", stu_tel=" + stu_tel
				+ ", stu_mail=" + stu_mail + ", stu_addr=" + stu_addr + ", stu_warn=" + stu_warn
				+ ", stu_credits_point=" + stu_credits_point + ", stu_graduation_minimum_credits="
				+ stu_graduation_minimum_credits + ", stu_grant=" + stu_grant + ", major_name=" + major_name + "]";
	}
	
}
